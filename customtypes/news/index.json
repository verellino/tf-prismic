{
  "id": "news",
  "label": "News",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Uid",
          "placeholder": ""
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "writer": {
        "type": "Text",
        "config": {
          "label": "Writer",
          "placeholder": ""
        }
      },
      "editor": {
        "type": "Text",
        "config": {
          "label": "Editor",
          "placeholder": ""
        }
      },
      "publishDate": {
        "type": "Timestamp",
        "config": {
          "label": "Publish Date",
          "placeholder": ""
        }
      },
      "section": {
        "type": "Select",
        "config": {
          "label": "Section",
          "placeholder": "",
          "options": [
            "Economy",
            "Government",
            "Technology"
          ]
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Category",
          "placeholder": "",
          "options": [
            "Investasi dan Keuangan",
            "Industri dan Perdagangan",
            "Akses dan Infrastruktur",
            "Keamanan dan Pertahanan",
            "Energi dan Lingkungan",
            "Inisiatif dan Pembangunan",
            "Kepentingan Publik",
            "Artificial Intelligence",
            "AR/VR",
            "Blockchain",
            "Cloud computing",
            "Data Analytics",
            "Internet of Things",
            "Metaverse",
            "Robotics"
          ]
        }
      },
      "categories": {
        "type": "Link",
        "config": {
          "label": "Categories",
          "select": "document",
          "customtypes": [
            "subcategory"
          ]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "text": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            },
            "quote": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}